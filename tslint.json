{
    "extends": [],
    "defaultSeverity": "warning",
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js"
        ]
    },
    "rules": {
        "ordered-imports": false,
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "no-string-literal": false
    }
}